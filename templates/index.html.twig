                        </button>
                        <div class="p-2 dropdown-menu dropdown-menu-right">
                            <form action="{{ path('feedback_send') }}" id="feedback-form" type="post" style="width: 250px">
                                <div class="form-group">
                                    <input name="nickname" type="text" class="form-control" placeholder="Nickname">
                                </div>
                                <div class="form-group">
                                    <textarea name="message" style="height: 150px" class="form-control" placeholder="Message"></textarea>
                                </div>

                                <label>* Content of the page will be send as well</label>
                                <button type="submit" class="btn btn-primary float-right">Send</button>
                            </form>
                        </div>
                        <script>
                            document.addEventListener("DOMContentLoaded", function(event) {

                                $('#feedback-form').submit(function (event) {
                                    event.preventDefault();

                                    var submitBtn = $("button[type=submit]", this);
                                    $(submitBtn).attr("disabled", true);
                                    var data = $(this).serializeArray();
                                    var formAction = $(this).attr('action');

                                    data.push(
                                        {
                                            name: 'html',
                                            value: document.documentElement.innerHTML
                                        }
                                    );

                                    $.ajax({
                                        type: "POST",
                                        url: formAction,
                                        data: data,
                                        success: function(data) {
                                            alert(data);
                                        },
                                        error: function() {
                                            alert('error');
                                        }
                                    });
                                })

                            });
                        </script>
                    </div>
